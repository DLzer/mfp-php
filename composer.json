        "myfitnesspal",
        "service",
        "scraper"
    ],
    "require": {
        "php": "^5.3 || ^7.0 || ^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dillon Lomnitzer",
            "email": "dillon.lomnitzer@gmail.com",
            "homepage": "https://raeinblack.com"
        }
    ],
    "autoload": {
        "psr-4": {
