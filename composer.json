{
    "name": "dlzer/mfp-php",
    "description": "PHP Libary for accessing MyFitnessPal diary data",
    "type": "library",
    "keywords": [
        "php",
        "mfp",
        "php-mfp",
        "api",
        "myfitnesspal",
        "service"
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dillon Lomnitzer",
            "email": "yodaesu@gmail.com",
            "homepage": "https://github.com/DLzer"
        }
    ],
    "autoload": {
        "psr-4": {
            "DLzer\\MFP\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/MfpTest"
    }
}
